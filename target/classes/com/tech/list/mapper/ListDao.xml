<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 컨트롤 시프트 x는 소문자를 대문자로 바꿔준다 -->

<mapper namespace="com.tech.list.dao.ListDao">
	<insert id="writereview">
     	INSERT INTO REVIEW VALUES (#{param1},'jaguar1178',5,#{param2},#{param3},REVIEWNUM.NEXTVAL,SYSDATE)
   </insert>
   
   <select id="cafereview" resultType="com.tech.cafein.dto.ReviewDto">
   		SELECT TITLE, CONTENT, IMG, RDATE FROM REVIEW ORDER BY 	REVIEWNUM DESC
   </select>
</mapper>